{
  "kind": "collectionType",
  "collectionName": "productos",
  "info": {
    "singularName": "producto",
    "pluralName": "productos",
    "displayName": "Producto",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "NameProduct": {
      "type": "string",
      "required": true,
      "unique": false,
      "minLength": 5
    },
    "Date": {
      "type": "date"
    },
    "Description": {
      "type": "richtext"
    },
    "Imagen": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Slug": {
      "type": "uid",
      "targetField": "NameProduct"
    },
    "Price": {
      "type": "decimal",
      "default": 0
    },
    "Resume": {
      "type": "richtext"
    },
    "Quatity": {
      "type": "integer"
    },
    "Discount": {
      "type": "integer",
      "max": 100,
      "min": 0
    },
    "metaDescription": {
      "type": "string"
    },
    "metaKeywords": {
      "type": "string"
    },
    "Color": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "tipo_producto": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::tipo-producto.tipo-producto"
    },
    "productos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::producto.producto"
    }
  }
}
